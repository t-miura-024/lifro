model RepMaxGoal {
    id         Int      @id @default(autoincrement())
    user       User     @relation(fields: [userId], references: [id])
    userId     Int
    exercise   Exercise @relation(fields: [exerciseId], references: [id])
    exerciseId Int
    weight     Float
    reps       Int
    startDate  DateTime
    endDate    DateTime
    createdAt  DateTime @default(now())
    updatedAt  DateTime @updatedAt
}
