model User {
    id                        Int                        @id @default(autoincrement())
    email                     String                     @unique
    createdAt                 DateTime                   @default(now())
    updatedAt                 DateTime                   @updatedAt
    exercises                 Exercise[]
    repMaxGoals               RepMaxGoal[]
    periodVolumeGoals         PeriodVolumeGoal[]
    exercisePeriodVolumeGoals ExercisePeriodVolumeGoal[]
    routines                  Routine[]
}
